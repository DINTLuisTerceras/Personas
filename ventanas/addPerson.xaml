<Window x:Class="Personas.ventanas.addPerson"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Personas.ventanas"
        mc:Ignorable="d"
        Title="addPerson" Height="120" Width="300" ShowInTaskbar="False" ResizeMode="NoResize" Icon="../assets/Awicons-Vista-Artistic-Search.ico">
    <StackPanel Margin="5">
        <TextBlock>Nacionalidad</TextBlock>
        <TextBox Text="{Binding Path=Nacionalidad}"></TextBox>
        <WrapPanel HorizontalAlignment="Right">
            <Button Margin="5" Width="100" Click="Button_Click" Command="{Binding Path=aceptarNacion}">Aceptar</Button>
            <Button Margin="5" Width="100">Cancelar</Button>
        </WrapPanel>
    </StackPanel>
</Window>
